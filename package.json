{
  "name": "frontend",
  "version": "0.1.0",
  "description": "Fou Frontend",
  "dependencies": {
    "babel-runtime": "^5.8.5",
    "boron": "^0.1.2",
    "breakpoint-sass": "^2.6.1",
    "classnames": "^2.1.3",
    "debug": "^2.2.0",
    "eventemitter3": "^1.1.1",
    "fluxible": "^0.5.1",
    "fluxible-addons-react": "^0.1.6",
    "halogen": "^0.1.10",
    "immutable": "^3.7.4",
    "inherits": "^2.0.1",
    "koa": "^0.21.0",
    "koa-logger": "^1.3.0",
    "koa-router": "^5.1.2",
    "koa-static": "^1.4.9",
    "material-ui": "^0.10.1",
    "normalize.css": "^3.0.3",
    "react": "^0.13.3",
    "react-document-title": "^1.0.2",
    "react-router": "^0.13.3",
    "react-tap-event-plugin": "^0.1.7",
    "serialize-javascript": "^1.0.0",
    "source-map-support": "^0.3.2",
    "superagent": "^1.2.0",
    "susy": "^2.2.5"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^2.2.0",
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.8.5",
    "babel-core": "^5.8.5",
    "babel-eslint": "^4.0.0",
    "babel-loader": "^5.3.2",
    "css-loader": "Aluxian/css-loader",
    "eslint": "^0.24.1",
    "eslint-loader": "^0.14.2",
    "eslint-plugin-react": "^3.0.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "handlebars": "^3.0.3",
    "handlebars-loader": "^1.1.0",
    "img-loader": "^1.2.0",
    "jest-cli": "facebook/jest#0.5.x",
    "node-sass": "^3.2.0",
    "nodemon": "^1.3.8",
    "nyan-progress-webpack-plugin": "^1.0.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.2.8",
    "sass-loader": "^1.0.3",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  },
  "scripts": {
    "babel-build": "rm -rf build && babel --out-dir build src",
    "babel-watch": "rm -rf build && (babel --watch --out-dir build src &) && sleep 2",
    "lint": "eslint src gulpfile.babel.js webpack.config.js && scss-lint src",
    "lint-watch": "nodemon --watch src --exec 'eslint src && scss-lint src'",
    "test": "npm run babel-build && jest",
    "test-watch": "npm run babel-watch && nodemon --watch build --exec 'jest'",
    "coverage": "npm run babel-build && jest --coverage",
    "coverage-watch": "npm run babel-watch && nodemon --watch build --exec 'jest --coverage'",
    "bundle-client": "webpack --config webpack.client.js --release",
    "bundle-server": "webpack --config webpack.server.js --release",
    "bundle": "UV_THREADPOOL_SIZE=100 rm -rf dist && mkdir dist && cp -rf public dist/public && npm run bundle-client && npm run bundle-server",
    "start": "node dist/server.js",
    "watch-client": "UV_THREADPOOL_SIZE=100 webpack-dev-server --config webpack.client.js --content-base http://0.0.0.0:9090 --inline --hot --host $HOSTNAME",
    "watch-server": "UV_THREADPOOL_SIZE=100 nodemon -w 'src/server.js' -w 'src/api' -w 'src/views' -x 'webpack --config webpack.server.js && node dist/server.js'",
    "browser-sync": "browser-sync start --proxy 0.0.0.0:9090"
  },
  "jest": {
    "testPathDirs": [
      "build"
    ],
    "unmockedModulePathPatterns": [
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FOUfashion/frontend.git"
  },
  "author": "Alexandru Rosianu <me@aluxian.com>",
  "license": "MIT"
}
