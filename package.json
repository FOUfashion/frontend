{
  "name": "frontend",
  "version": "0.1.0",
  "description": "Fou Frontend",
  "dependencies": {
    "babel-runtime": "^5.8.3",
    "classnames": "^2.1.3",
    "eventemitter3": "^1.1.1",
    "express": "^4.13.1",
    "fastclick": "^1.0.6",
    "flux": "^2.0.3",
    "front-matter": "^1.0.0",
    "jade": "^1.11.0",
    "purecss": "^0.6.0",
    "react": "^0.13.3",
    "source-map-support": "^0.3.2",
    "superagent": "^1.2.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.8.3",
    "babel-core": "^5.8.3",
    "babel-eslint": "^3.1.26",
    "babel-loader": "^5.3.2",
    "browser-sync": "^2.8.0",
    "css-loader": "Aluxian/css-loader",
    "eslint": "^0.24.1",
    "eslint-loader": "^0.14.2",
    "eslint-plugin-react": "^3.0.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "gulp": "^3.9.0",
    "gulp-changed": "^1.2.1",
    "gulp-if": "^1.2.5",
    "gulp-load-plugins": "^1.0.0-rc",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^1.2.3",
    "gulp-util": "^3.0.6",
    "handlebars": "^3.0.3",
    "handlebars-loader": "^1.1.0",
    "jest-cli": "facebook/jest#0.5.x",
    "node-sass": "^3.2.0",
    "nodemon": "^1.3.8",
    "run-sequence": "^1.1.2",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.1"
  },
  "scripts": {
    "build": "rm -rf build && babel --out-dir build src",
    "watch": "rm -rf build && (babel --watch --out-dir build src &) && sleep 2",
    "lint": "eslint src gulpfile.babel.js webpack.config.js",
    "lint-watch": "nodemon --watch src --exec 'eslint src'",
    "test": "npm run build && jest",
    "test-watch": "npm run watch && nodemon --watch build --exec 'jest'",
    "coverage": "npm run build && jest --coverage",
    "coverage-watch": "npm run watch && nodemon --watch build --exec 'jest --coverage'",
    "dist": "rm -rf dist && gulp dist --release",
    "start": "node dist/server.js",
    "start-dev": "rm -rf dist && gulp serve",
    "start-sync": "rm -rf dist && gulp sync"
  },
  "jest": {
    "testPathDirs": [
      "build"
    ],
    "unmockedModulePathPatterns": [
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FOUfashion/frontend.git"
  },
  "author": "Alexandru Rosianu <me@aluxian.com>",
  "license": "MIT"
}
