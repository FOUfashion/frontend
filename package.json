    "koa-json-body": "^3.0.0",
    "koa-logger": "^1.3.0",
    "koa-mount": "^1.3.0",
    "koa-proxy": "^0.3.0",
    "koa-redis": "^1.0.0",
    "koa-router": "^5.1.2",
    "koa-static": "^1.4.9",
    "loaders.css": "^0.1.1",
