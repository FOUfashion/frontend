    "superagent-bluebird-promise": "^2.0.2",
    "susy": "^2.2.5",
    "timeout-transition-group": "^1.0.3"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^2.2.0",
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.8.12",
    "babel-core": "^5.8.14",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "browser-sync": "^2.8.1",
    "css-loader": "Aluxian/css-loader",
    "eslint": "^0.24.1",
    "eslint-loader": "^0.14.2",
    "eslint-plugin-react": "^3.1.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "handlebars": "^3.0.3",
    "handlebars-loader": "^1.1.0",
    "img-loader": "^1.2.0",
    "jest-cli": "facebook/jest#0.5.x",
    "json-loader": "^0.5.2",
    "node-sass": "^3.2.0",
    "nodemon": "^1.4.0",
    "nyan-progress-webpack-plugin": "^1.0.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.2.8",
    "sass-loader": "^1.0.3",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1",
    "webpack-error-notification": "^0.1.4"
  },
  "scripts": {
    "babel-build": "rm -rf build && babel --out-dir build src",
    "babel-watch": "rm -rf build && (babel --watch --out-dir build src &) && sleep 2",
    "lint": "eslint src gulpfile.babel.js webpack.config.js && scss-lint src",
    "lint-watch": "nodemon --watch src --exec 'eslint src && scss-lint src'",
    "test": "npm run babel-build && jest --verbose",
    "test-watch": "npm run babel-watch && nodemon --watch build --exec 'jest'",
    "coverage": "npm run babel-build && jest --coverage",
    "coverage-watch": "npm run babel-watch && nodemon --watch build --exec 'jest --coverage'",
    "bundle-client": "webpack --config webpack.client.js --release",
    "bundle-server": "webpack --config webpack.server.js --release",
    "bundle": "rm -rf dist && mkdir dist && cp -rf public dist/public && npm run bundle-client && npm run bundle-server",
    "start": "node dist/server.js",
    "watch-client": "webpack-dev-server --config webpack.client.js --content-base \"http://0.0.0.0:$FRONTEND_PORT\"  --inline --hot --host $FRONTEND_HOSTNAME --port $FRONTEND_WP_PORT",
    "watch-server": "nodemon -w 'src/server.js' -w 'src/api' -w 'src/views' -x 'webpack --config webpack.server.js && node dist/server.js'",
    "browser-sync": "browser-sync start --proxy \"0.0.0.0:$FRONTEND_PORT\""
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor",
    "testDirectoryName": "build",
    "testPathDirs": [
      "build"
    ],
    "testFileExtensions": [
      "spec.js"
    ],
    "unmockedModulePathPatterns": [
      "/node_modules/"
    ]
  },
  "jest-webpack-config": "webpack.client.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:FOUfashion/frontend.git"
  },
  "author": "Alexandru Rosianu <me@aluxian.com>",
  "license": "MIT"
}
